digraph {
	node [shape=circle]
	start [label="" color=black fillcolor=black shape=circle style=filled width=0.1]
	q0 [label=q0]
	q1 [label=q1]
	q2 [label=q2 shape=doublecircle]
	start -> q0
	q0 -> q1 [label="C.O.S"]
	q1 -> q2 [label=";"]
	q1 -> q1 [label="C.O.S"]
	label="Cantidad de ; en el codigo = 1"
}
